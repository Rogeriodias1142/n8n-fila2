impos\nreturn items;\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[1320,0],"id":"d31d587a-273d-4a0a-bae2-e9daa844310d","name":"Limpar Texto da Descrição"},{"parameters":{"operation":"get","id":{"__rl":true,"value":"={{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}","mode":"id"},"additionalFields":{}},"type":"n8n-nodes-base.trello","typeVersion":1,"position":[1980,0],"id":"0a93d79e-4f68-4fea-95ed-88817f1f0399","name":"Obter Detalhes do Cartão","credentials":{"trelloApi":{"id":"rpm3uMep21rqpid6","name":"Trello account"}}},{"parameters":{"operation":"update","id":{"__rl":true,"value":"={{ $json.id }}","mode":"id"},"updateFields":{"desc":"=\"título\": \"{{ $item(\"0\").$node[\"Consolidar Dados do Cartão\"].json[\"output\"][\"0\"][\"título\"] }}\"\n\"pasta\": \"{{ $item(\"0\").$node[\"Consolidar Dados do Cartão\"].json[\"output\"][\"0\"][\"pasta\"] }}\",\n\"id\": \"{{ $item(\"0\").$node[\"Enviar para API de Upload do Google Drive\"].json[\"file\"][\"id\"] }}\",\n\"url\": \"{{ $item(\"0\").$node[\"Enviar para API de Upload do Google Drive\"].json[\"file\"][\"webViewLink\"] }}\",\n\"data\": \"{{ $item(\"0\").$node[\"Calcular Data de Publicação\"].json[\"datePlusOne\"] }}\",\n"}},"type":"n8n-nodes-base.trello","typeVersion":1,"position":[2200,0],"id":"8a724845-57ac-4020-a476-6322fcd02b0b","name":"Atualizar Cartão com Link do Vídeo","credentials":{"trelloApi":{"id":"rpm3uMep21rqpid6","name":"Trello account"}}},{"parameters":{"command":"=rm -f \"/tmp/{{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}concatenated_output.mp4\" \\       \"/tmp/{{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}vídeo1.mp4\" \\       \"/tmp/{{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}vídeo2.mp4\" \\       \"/tmp/{{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}vídeo3.mp4\" \\       \"/tmp/{{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}vídeo4.mp4\" \\       \"/tmp/{{