"Preparar para Concatenação"},{"parameters":{"url":"={{ $json.body }}","options":{}},"type":"n8n-nodes-base.httpRequest","typeVersion":4.2,"position":[440,-60],"id":"2737d3eb-4e6c-47b8-bb07-4d8c5b96da94","name":"Baixar Arquivo de Vídeo"},{"parameters":{"resource":"file","path":"/tmp","options":{"fileName":"={{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}vídeo{{ $runIndex+1 }}.mp4"}},"type":"n8n-nodes-base.ssh","typeVersion":1,"position":[660,-60],"id":"8d2b5706-77af-4a9b-9b29-73b9614451b9","name":"Salvar Vídeo no Servidor","credentials":{"sshPassword":{"id":"704P5Oy4Ul61S643","name":"SSH Password account"}}},{"parameters":{"jsCode":"// Nó “Code” do n8n — gera um script de concatenação para 10 vídeos com prefixo dinâmico\nfunction concatVideos(items) {\n  // 1) captura o id dinâmico\n  const id = $('Selecionar Próximo Cartão').first().json.id;\n  const prefix = `/tmp/${id}`;\n\n  // 2) monta lista de vídeos de /tmp/[AQUI]vídeo1.mp4 até /tmp/[AQUI]vídeo10.mp4\n  const videoPaths = [];\n  for (let i = 1; i <= 10; i++) {\n    videoPaths.push(`${prefix}vídeo${i}.mp4`);\n  }\n\n  // 3) define o path de saída dinâmico\n  const outputPath = `${prefix}concatenated_output.mp4`;\n\n  // 4) monta o script bash\n  const ffmpegScript = `\n#!/bin/bash\n# Remove lista antiga e cria nova\nrm -f concat_list.txt\ntouch concat_list.txt\n\n# Adiciona cada vídeo à lista\n${videoPaths.map(path => `echo \"file '${path}'\" >> concat_list.txt`).join('\\n')}\n\n# Concatena os vídeos em um único arquivo\nffmpeg -y -f concat -safe 0 -i concat_list.txt -c copy ${outputPath}\n\necho \"Vídeo finalizado: ${outputPath}\"\n`;\n\n  return [\n    {\n      json: {\n        ffmpegScript,\n        videoPaths,\n        outputPath,\n      },\n    },\n  ];\n}\n\n// Executa a função com todos os itens de entrada\nreturn concatVideos($input.all());\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[660,-260],"id":"bd211bad-4467-4acb-b3f7-0d01cb28928e","name":"Gerar Script de Concatenação"},{"parameters":{"command":"={{ $json.ffmpegScript }}"},"type":"n8n-nodes-base.ssh","typeVersion":1,"position":[880,-260],"id":"982ecb92-504d-4dab-bb84-f67bcc0dbc9c","name":"Executar FFmpeg para Concatenar","credentials":{"sshPassword":{"id":"704P5Oy4Ul61S643","name":"SSH Password account"}}},{"parameters":{"options":{}},"type":"n8n-nodes-base.dateTime","typeVersion":2,"position":[1540,-260],"id":"1538681c-189d-4a2f-9b91-7ef964f086f9","name":"Obter Data Atual"},{"parameters":{"jsCode":"// 1) pega a data/hora atual\nconst now = new Date();\n\n// 2) adiciona 1 dia\nnow.setDate(now.getDate() + 1);\n\n// 3) formata como \"YYYY-MM-DD\"\nconst formatted = now.toISOString().split('T')[0];\n\n// 4) devolve um único item com a nova data\nreturn [\n  {\n    json: {\n      datePlusOne: formatted\n    }\n  }\n];\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[1760,-260],"id":"48943033-e499-4b23-b632-3b7f2b2a9d45","name":"Calcular Data de Publicação"},{"parameters":{"assignments":{"assignments":[{"id":"8dcb9efd-77f8-4044-a774-8d9a92767cf8","name":"output[0].seo","value":"={{ $item(\"0\").$node[\"Consolidar Dados do Cartão\"].json[\"output\"][\"0\"][\"seo\"] }}","type":"string"}]},"options":{}},"type":"n8n-nodes-base.set","typeVersion":3.4,"position":[1980,-260],"id":"def9703f-f49c-4826-be38-6b4a6a493add","name":"Preparar Descrição do Vídeo"},{"parameters":{"jsCode":"// Itera sobre cada item recebido\nitems.forEach(item => {\n  const outputArr = item.json.output;\n  // Garante que exista o array e o índice 0 com a propriedade seo\n  if (Array.isArray(outputArr) && outputArr[0] && typeof outputArr[0].seo === 'string') {\n    let text = outputArr[0].seo;\n    // remove quebras de linha reais (\\r, \\n) e literais (\\n, \\r)\n    text = text\n      .replace(/[\\r\\n]+/g, ' ')   // quebras reais\n      .replace(/\\\\[rn]+/g, ' ');  // sequências literais\n    // atualiza o valor no JSON de saída\n    outputArr[0].seo = text;\n  }\n});\n\n// Retorna os items já limpos\nreturn items;\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[2200,-260],"id":"c2eb558c-b618-477e-a2c1-0690f2f91cab","name":"Limpar Texto da Descrição"},{"parameters":{"method":"POST","url":"={{ $item(\"0\").$node[\"Edit Fields\"].json[\"url\"] }}upload","authentication":"predefinedCredentialType","nodeCredentialType":"youTubeOAuth2Api","sendBody":true,"specifyBody":"json","jsonBody":"={\n\"filePath\": \"/tmp/{{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}concatenated_output.mp4\",\n\"title\": \"{{ $item(\"0\").$node[\"Consolidar Dados do Cartão\"].json[\"output\"][\"0\"][\"título\"] }}\",\n\"description\": \"{{ $json.output[0].seo }}\",\n\"categoryId\": \"22\",\n\"defaultLanguage\":  \"it-IT\",\n\"defaultAudioLanguage\":  \"it-IT\",\n\"privacyStatus\": \"private\",\n\"license\": \"youtube\",\n\"embeddable\": true,\n\"publicStatsViewable\": true,\n\"madeForKids\": false,\n\"selfDeclaredMadeForKids\": false,\n\"containsSyntheticMedia\": false,\n\"publishAt\": \"{{ $('Calcular Data de Publicação').item.json.datePlusOne }}T05:00:00Z\",\n\"recordingDetails\": {\n    \"recordingDate\": \"{{ $('Calcular Data de Publicação').item.json.datePlusOne }}T05:00:00Z\",\n    \"locationDescription\": \"Veneza, Italia\"\n  },\n  \"contentDetails\": {\n    \"caption\": \"false\",\n    \"definition\": \"hd\",\n    \"dimension\": \"2d\",\n    \"licensedContent\": true\n  }\n}","options":{}},"type":"n8n-nodes-base.httpRequest","typeVersion":4.2,"position":[2420,-260],"id":"7e297909-3637-48c7-a00f-abd2cacac66f","name":"Enviar para API de Upload","credentials":{"youTubeOAuth2Api":{"id":"kjnti1FjNerdM4iz","name":"@Storieeracconti"}}},{"parameters":{"operation":"get","id":{"__rl":true,"value":"={{ $item(\"0\").$node[\"Selecionar Próximo Cartão\"].json[\"id\"] }}","mode":"id"},"additionalFields":{}},"type":"n8n-nodes-base.trello","typeVersion":1,"position":[2640,-260],"id":"c3324749-08d1-46d6-9832-0c7dd6fa55cf","name":"Obter Detalhes do Cartão","crede