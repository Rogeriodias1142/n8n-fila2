rorOutput"},{"parameters":{"jsCode":"// pega o id dinâmico\nconst id = $('Processar Próximo Cartão').first().json.id;\nconst prefix = `/tmp/${id}`;\n\n// monta a lista de WAVs com o prefixo\nconst files = Array.from({ length: 6 }, (_, i) => `${prefix}tmpaudio${i+1}.wav`);\n\n// concat list e output também ganham o prefixo\nconst listFilePath = `${prefix}_concat_list.txt`;\nconst outputPath   = `${prefix}combined_output.mp3`;\n\n// gera o conteúdo do arquivo de lista\nconst fileListContent = files.map(f => `file '${f}'`).join('\\n');\n\n// monta o script bash\nconst bashScript = [\n  `rm -f ${outputPath}`,                                   // limpa MP3 antigo\n  `cat <<EOF > ${listFilePath}`,\n  fileListContent,\n  `EOF`,\n  `ffmpeg -y -f concat -safe 0 -i ${listFilePath} -c:a libmp3lame -qscale:a 2 ${outputPath}`, // concat + MP3\n  `rm -f ${files.join(' ')}`                                // apaga os WAVs\n].join('\\n');\n\nreturn [\n  {\n    json: {\n      bashScript,\n    },\n  },\n];\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[2640,540],"id":"51879ad5-94a0-4025-a65b-3878686a7d38","name":"Preparar Script de Concatenação"},{"parameters":{"command":"={{ $json.bashScript }}"},"type":"n8n-nodes-base.ssh","typeVersion":1,"position":[2860,540],"id":"8736d3af-304c-4403-94c6-e8351ff2543f","name":"Executar Concatenação de Áudio","credentials":{"sshPassword":{"id":"704P5Oy4Ul61S643","name":"SSH Password account"}}},{"parameters":{"resource":"file","operation":"download","path":"=/tmp/{{ $item(\"0\").$node[\"Processar Próximo Cartão\"].json[\"id\"] }}combined_output.mp3","options":{}},"type":"n8n-nodes-base.ssh","typeVersion":1,"position":[3080,540],"id":"07404182-5d26-43ae-8f5c-d7c9e6465c2c","name":"Baixar Áudio Combinado","credentials":{"sshPassword":{"id":"704P5Oy4Ul61S643","name":"SSH Password account"}}},{"parameters":{"name":"output","driveId":{"__rl":true,"value":"My Drive","mode":"list","cachedResultName":"My Drive","cachedResultUrl":"https://drive.